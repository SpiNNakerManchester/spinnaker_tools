MEMORY
{
    ITCM : ORIGIN = 0, LENGTH = 0x7f00
}


SECTIONS
{
    ITCM_BASE = 0x00000000;

    . = ITCM_BASE;

    RO_DATA :
    {
          * (boot_aplx);
    } > ITCM

    . = ALIGN (4);

    RO_BASE = ITCM_BASE;
}
